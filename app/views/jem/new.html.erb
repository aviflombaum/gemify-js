<%= form_for(@jem, :html => { :multipart => true }) do |f| %>
  
  <%= f.label :name, 'Name' %>
  </br>
  <%= f.text_field :name %>
  </br>
  <%= f.label :github, 'Github Repository' %>
  </br>
  <%= f.text_field :github %>
  </br>
  <%= f.fields_for :scripts do |p| %>
    <%= p.label :file %><br>
    <%= p.file_field :file, :multiple => true, name: "scripts[file][]" %>
  <% end %>
  </br>
  <%= f.submit 'Create Gem'%>
<% end %>
