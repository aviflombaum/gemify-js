<%= form_for(@jem, :html => { :multipart => true }) do |f| %>

  <%= f.label :name, 'Name' %></br>
  <%= f.text_field :name %></br>

  <%= f.label :author, 'Author' %></br>
  <%= f.text_field :author %></br>

  <%= f.label :version_number, 'Version Number' %></br>
  <%= f.text_field :version_number %></br>

  <%= f.label :description, 'Description' %></br>
  <%= f.text_field :description %></br>

  <%= f.label :homepage, 'Homepage' %></br>
  <%= f.text_field :homepage %></br>

  <%= f.label :email, 'Email Address' %></br>
  <%= f.text_field :email %></br>

  <%= f.label :github, 'Github Repository' %></br>
  <%= f.text_field :github %></br>

  <%= f.submit 'Create Gem'%>
<% end %>
